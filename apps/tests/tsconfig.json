{
    "extends": "tsconfig/base.json",
    "compilerOptions": {
        "module": "commonjs",
        "outDir": "dist",
        "noEmit": false,
        "moduleResolution": "node",
        "typeRoots": ["./interfaces"],
        "resolveJsonModule": true
    },
    "include": ["src/**/*.ts", "interfaces/*.d.ts"],
    "exclude": ["node_modules"]
}
