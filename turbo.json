{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", ".next/**"]
        },
        "lint": {
            "dependsOn": [],
            "outputs": [],
            "env": ["NODE_ENV", "PORT", "HOST"]
        },
        "dev": {
            "cache": false
        }
    },
    "globalEnv": ["NODE_ENV", "POST", "HOST", "ELECTRON"],
    "globalDependencies": []
}
