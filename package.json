{
    "name": "my-turborepo",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "dev:main": "turbo run dev --filter=main",
        "dev:agent": "turbo run dev --parallel",
        "dev:agent:main": "turbo run dev --filter=agent",
        "dev:agent:gui": "turbo run dev --filter=web"
    },
    "devDependencies": {
        "eslint-config-custom": "*",
        "prettier": "latest",
        "turbo": "latest"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "packageManager": "npm@7.24.0"
}
